      #9'left join city cp on f.post_city_id = cp.city_id '
      'where upper(f.name) like upper(:firmName) and'
      '          upper(cp.name) like upper(:cityPostName) and'
      '          upper(cj.name) like upper(:cityJurName)'
      'order by f.firm_id')
